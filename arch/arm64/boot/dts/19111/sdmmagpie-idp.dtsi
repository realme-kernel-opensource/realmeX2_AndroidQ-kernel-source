    oppo_secure_common {
        compatible = "oppo,secure_common";
    };
    //#endif //VENDOR_EDIT
    // #ifdef VENDOR_EDIT
    /* ziqing.guo@BSP.Fingerprint.Basic 2019-07-18 add for euclid */
    /* oujinrong@BSP.Fingerprint.Basic 2019-09-03 add for power list */
